#include <iostream>

using namespace std;

int main()
{
    int x,y;
cout<<"please enter two integer number :";
cin>>x>>y;
if(x==y){
cout<<"x is equal with y";
}
if(x!=y){
cout<<"x is not equal with y"<<endl;
}
if(x>y){
cout<<"x is greater than y"<<endl;
}
if(x<y){
cout<<"x is lower than y"<<endl;
}
if(x>=y){
 cout<<"x is greater or equal with y"<<endl;
}
if(x<=y){
cout<<"x is lower or equal with y"<<endl;
}
    return 0;
}
